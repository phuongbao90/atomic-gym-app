{
  "name": "gym-app-turbo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo watch dev",
    "lint": "turbo lint",
    "postinstall": "patch-package",
    "watch-packages": "turbo run watch-packages",
    "hacky-build-ios": "cd apps/mobile && yarn release --platform ios",
    "check-packages-version": "npx syncpack list",
    "_": "------------------------------------------",
    "dev:web": "      turbo watch dev --filter=@repo/api --filter=@repo/web --filter=@repo/app",
    "dev:dashboard": "turbo watch dev --filter=@repo/api --filter=@repo/dashboard --filter=@repo/app",
    "dev:mobile": "   turbo watch dev --filter=@repo/api --filter=@repo/mobile --filter=@repo/app-config --filter=@repo/app",
    "dev:api": "      turbo run start:debug --filter=@repo/api",
    "__": "-----------------------------------------",
    "build:mobile": "turbo run build --filter=@repo/mobile"
  },
  "devDependencies": {
    "dotenv-mono": "^1.3.14",
    "turbo": "^2.4.4",
    "typescript": "5.7.2",
    "@biomejs/biome": "1.9.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": ["apps/*", "packages/*"]
}
