{
  "name": "gym-app-turbo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo watch dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "patch-package",
    "watch-packages": "turbo run watch-packages",
    "hacky-build-ios": "cd apps/mobile && yarn release --platform ios",
    "check-packages-version": "npx syncpack list",
    "_": "------------------------------------------",
    "dev:web": "      turbo watch dev --filter=@repo/api --filter=@repo/web",
    "dev:dashboard": "turbo watch dev --filter=@repo/api --filter=@repo/dashboard",
    "dev:mobile": "   turbo watch dev --filter=@repo/api --filter=@repo/mobile --filter=@repo/app-config",
    "dev:api": "      turbo run start:debug --filter=@repo/api",
    "__": "-----------------------------------------"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.3.0",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
