{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {}
    },
    "staging": {
      "developmentClient": false,
      "distribution": "internal",
      "resourceClass": "large",
      "ios": {
        "image": "latest"
      },
      "env": {}
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "resourceClass": "large",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      },
      "env": {}
    }
  }
}
