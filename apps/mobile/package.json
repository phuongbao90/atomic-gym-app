  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf ios && rm -rf android && rm -rf .expo && rm -rf .turbo",
    "lint": "biome check --write .",
    "copy-env-to-eas": "ts-node ./scripts/copy-env-to-eas.ts ",
    "postinstall": "cd ../.. & patch-package",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "_": "----------------------------------------------------------------------------",
    "start": "   cross-env NODE_ENV=$(node ./scripts/read-env.mjs)  npx expo start --dev-client --port 8086 -c",
    "predebug": "cross-env NODE_ENV=$(node ./scripts/read-env.mjs)  EXPO_NO_GIT_STATUS=1     npx expo prebuild    --platform $(node ./scripts/read-platform.mjs) --clean",
    "debug": "   cross-env NODE_ENV=$(node ./scripts/read-env.mjs)  npx expo run:$(node ./scripts/read-platform.mjs) --port 8086",
    "build": "yarn copy-env-to-eas && eas build --local --clear-cache",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
