{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "nodenext",
    "module": "nodenext"
  },
  "include": [
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/components/__tests__/header-test.jsx",
    "setupTests.js"
  ]
}
