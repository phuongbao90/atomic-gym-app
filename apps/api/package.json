{
  "name": "api",
  "version": "0.0.1",
  "description": "backend for gym app",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "nest build",
    "start:": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "__": "----------------------------------------",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "_migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset",
    "migrate:resolve": "npx prisma migrate resolve",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "reset-db": "npx prisma db push --force-reset && yarn seed && yarn prisma:generate",
    "_": "-----------------------------------------",
    "dev": "      NODE_ENV=development nest start --watch",
    "debug_api": "NODE_ENV=development nest start --watch",
    "start": "    NODE_ENV=development nest start --watch",
    "dev:ssl": "  NODE_ENV=development nest start --watch --ssl"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@ngneat/falso": "^7.3.0",
    "@prisma/client": "^6.5.0",
    "@types/passport-jwt": "^4.0.1",
    "app-config": "*",
    "bcrypt": "5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv-cli": "^8.0.0",
    "express-session": "^1.18.1",
    "joi": "^17.13.3",
    "minio": "^8.0.5",
    "nestjs-minio": "^2.6.2",
    "nestjs-minio-client": "^2.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "prisma-class-validator-generator": "^5.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/minio": "^7.1.1",
    "@types/multer": "1.4.12",
    "@types/node": "^22.13.13",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "prisma": "^6.5.0",
    "prisma-generator-typescript-interfaces": "^2.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.2",
    "typescript-config": "*",
    "zod": "^3.24.2"
  }
}
